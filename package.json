{
  "name": "joshduff.com",
  "engines": {
    "node": ">=7.6.0"
  },
  "alias": "joshduff.com",
  "private": true,
  "version": "1.0.0",
  "description": "JoshDuff.com",
  "scripts": {
    "generate": "generate-noddity --root=content --output=public --template=index.html --extension=md '*.md' --filter=./noddity-filter.js --data=./noddity-data.json --feed=./noddity-feed.json && cpy content/images public/images",
    "test": "npm run lint",
    "start": "node server/server.js",
    "lint": "noddity-linter \"./content/\" --useIndex=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/joshduff.com.git"
  },
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/joshduff.com/issues"
  },
  "homepage": "https://github.com/TehShrike/joshduff.com#readme",
  "dependencies": {
    "polkadot": "^0.2.0",
    "polkadot-router": "^2.0.0"
  },
  "devDependencies": {
    "cpy-cli": "^2.0.0",
    "noddity-generator-cli": "^1.3.0",
    "noddity-linter": "^1.1.0"
  }
}
