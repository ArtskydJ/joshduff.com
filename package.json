{
  "name": "joshduff.com",
  "engines": {
    "node": ">=7.6.0"
  },
  "alias": "joshduff.com",
  "private": true,
  "version": "1.0.0",
  "description": "JoshDuff.com",
  "scripts": {
    "generate": "generate-noddity --root=content --output=docs --template=index.html --extension=html '*.md' --filter=./noddity-filter.js --data=./noddity-data.json --feed=./noddity-feed.json && cpy content/images docs/images",
    "test": "npm run lint",
    "start": "sirv docs --dev",
    "lint": "noddity-linter \"./content/\" --useIndex=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/joshduff.com.git"
  },
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/joshduff.com/issues"
  },
  "homepage": "https://github.com/TehShrike/joshduff.com#readme",
  "devDependencies": {
    "cpy-cli": "^3.1.1",
    "noddity-generator-cli": "^1.3.1",
    "noddity-linter": "^1.1.0",
    "sirv-cli": "^1.0.12"
  }
}
